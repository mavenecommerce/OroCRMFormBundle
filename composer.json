{
    "name": "mavenecommerce/OroCRMFormBundle",
    "description": "OroCRM Package, Embed form",
    "homepage": "https://github.com/mavenecommerce/OroCRMFormBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Andrii Korolov",
            "homepage": "https://www.mavenecommerce.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Maven\\Bundle\\FormBundle": "src/"
        }
    },
    "target-dir": "src/Maven/Bundle/FormBundle",
    "require": {
        "php": ">=5.4.4",
        "oro/platform": "1.x-dev",
        "oro/crm": "1.x-dev",
        "mavenecommerce/oro-crm-embedded-form-bundle": "1.x-dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
